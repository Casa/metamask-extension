{
  "name": "node-example",
  "version": "1.0.0",
  "description": "An AWS Lambda function in Typescript",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint": "esprint check",
    "lint:eslint:fix": "esprint check --fix",
    "test": "NODE_ENV=test jest --runInBand",
    "test:debug": "NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:summary": "NODE_ENV=test TEST_REPORT=summary jest --runInBand --silent --verbose",
    "test:coverage": "NODE_ENV=test jest --runInBand --coverage true",
    "test:coverage:open": "open coverage/lcov-report/index.html",
    "doc": "yarn swagger-jsdoc -d src/constants/swagger-def.json src/*/*.ts -o swagger.json",
    "prebuild": "rm -rf ./build",
    "build": "yarn doc && yarn build:check && yarn build:bundle && yarn build:migrations && yarn build:info && yarn build:zip",
    "build:check": "tsc --project tsconfig.json",
    "build:bundle": "esbuild --bundle --minify --keep-names --sourcemap --sources-content=false --define:process.env.NODE_ENV=\\\"production\\\" --target=node16 --platform=node --outfile=build/lambda.js src/lambda.ts",
    "build:migrations": "esbuild src/migrations/*.ts --bundle --target=node16 --platform=node --outdir=build/migrations",
    "build:info": "bash ./scripts/build-info.sh",
    "build:zip": "cd build && cp ../swagger.json . && zip -q -r lambda.zip lambda.js lambda.js.map info.json swagger.json migrations/*",
    "deploy": "bash ./scripts/lambda-deploy.sh",
    "invoke": "bash ./scripts/lambda-invoke.sh",
    "db:init": "bash ./scripts/db-init.sh",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:all:undo"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn build"
    }
  },
  "engines": {
    "node": "16.16.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1161.0",
    "datadog-winston": "^1.5.1",
    "dotenv": "^16.0.1",
    "lambda-api": "^0.11.2",
    "patch-package": "^6.4.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pg-native": "^3.0.1",
    "postinstall-postinstall": "^2.1.0",
    "sequelize": "^6.29.0",
    "swagger-jsdoc": "^6.2.5",
    "umzug": "^3.2.1",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/aws-lambda": "^8.10.68",
    "@types/datadog-winston": "^1.0.5",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.6.5",
    "@types/sinon": "^10.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/umzug": "^2.3.3",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "esbuild": "^0.25.0",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.4",
    "esprint": "^3.3.0",
    "husky": "^8.0.1",
    "jest": "^27.0.4",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.4.1",
    "sinon": "^13.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
